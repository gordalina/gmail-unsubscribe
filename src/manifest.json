{
  "name": "Gmail Unsubscribe",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [{
    "matches": ["https://mail.google.com/*"],
    "js": ["contentScript.bundle.js"]
  }],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
