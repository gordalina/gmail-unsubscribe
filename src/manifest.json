{
  "name": "Gmail Unsubscribe",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "commands": {
    "unsubscribe": {
      "description": "Unsubscribe from mailing list",
      "suggested_key": {
        "default": "Ctrl+U"
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content_script.bundle.js"
      ]
    }
  ],
  "permissions": [
    "active_tab"
  ],
  "manifest_version": 2
}
