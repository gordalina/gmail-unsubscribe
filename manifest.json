{
  "manifest_version": 3,
  "name": "Gmail Unsubscribe",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "service_worker": "service_worker.bundle.js"
  },
  "commands": {
    "unsubscribe": {
      "description": "Unsubscribe from mailing list",
      "suggested_key": {
        "default": "Alt+U",
        "mac": "Command+U"
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content_script.bundle.js"
      ]
    }
  ]
}
